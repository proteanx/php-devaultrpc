{
    "name": "protean/php-devaultrpc",
    "type": "library",
    "description": "DeVault JSON-RPC client based on GuzzleHttp",
    "keywords": ["devault","api","jsonrpc","guzzle"],
    "homepage": "https://github.com/proteanx/php-devaultrpc",
    "license": "MIT",
    "authors": [
        {
            "name": "Proteus Xists",
            "email": "proteus@devault.cc",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 | ^8.0"
    },
    "autoload": {
        "psr-4" : {
            "Protean\\DeVault\\": "src"
        },
        "files" : [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4" : {
            "Protean\\DeVault\\Tests\\": "tests"
        }
    }
}
